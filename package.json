    "watch": "npm-watch local",
    "local": "npx @google-cloud/functions-framework --port=4000 --target=http",
    "webpack:watch": "webpack --watch",
    "lint": "tslint --project tsconfig.json \"**/*.ts\"",
    "test": "jest --watchAll --colors --coverage"
